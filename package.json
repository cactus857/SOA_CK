{
    "name": "natours",
    "version": "2.0.0",
    "description": "Tour booking website built with Node.js",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "start:dev": "cross-env NODE_ENV=development nodemon server.js",
        "start:prod": "cross-env NODE_ENV=production node server.js",
        "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js",
        "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js"
    },
    "keywords": [
        "tours",
        "booking",
        "nodejs"
    ],
    "author": "MAES-Pyramids",
    "license": "ISC",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "axios": "^1.6.5",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "html-to-text": "^9.0.5",
        "jsonwebtoken": "^9.0.2",
        "mapbox-gl": "^2.15.0",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "nodemailer-mailgun-transport": "^2.1.5",
        "openai": "^6.9.1",
